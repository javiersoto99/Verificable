@model IEnumerable<UAndes.ICC5103._202301.Models.Inscripcion>

@{
    ViewBag.Title = "Index";
}

<h2>Inscripciones</h2>

<p>
    @Html.ActionLink("+ Nueva Inscripción", "Create")
</p>
<table class="table">
    <thead>
        <th>
            N° Atención
        </th>
        <th>
            Fecha Inscripción
        </th>
        <th>
            Fojas
        </th>
        <th>
            N° Inscripción
        </th>
        <th>
            Acciones
        </th>
    </thead>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @item.Creacion.ToShortDateString()
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fojas)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Numero_inscripcion)
        </td>
        <td>
            <button class="btn btn-default">@Html.ActionLink("Ver", "Details", new { id = item.Id })</button>
            
        </td>
    </tr>
}

</table>
